{
  "name": "Language Study",
  "description": "A helper extension for my language study app",
  "version": "0.1",
  "permissions": [
    "http://ceden.ml.cmu.edu:8002/",
    "contextMenus",
    "activeTab"
  ],
  "commands": {
    "definition": {
      "suggested_key": {
        "default": "Alt+D"
      },
      "description": "Get Definition"
    },
    "submit_as_read": {
      "suggested_key": {
        "default": "Alt+S"
      },
      "description": "Submit as read"
    }
  },
  "background": {
    "scripts": [
      "jquery-1.9.1.min.js",
      "scripts.js"
    ]
  },
  "options_page": "options.html",
  "manifest_version": 2
}
